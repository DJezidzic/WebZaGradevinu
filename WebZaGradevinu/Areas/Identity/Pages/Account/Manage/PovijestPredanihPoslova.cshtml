@page
@model PovijestPredanihPoslovaModel
@{
    ViewData["Title"] = "Povijest Predanih Poslova";
    ViewData["ActivePage"] = ManageNavPages.PovijestPredanihPoslova;
}

<h4>@ViewData["Title"]</h4>
<div class="row">
    <div class="col-md-6">
        <form id="profile-form" method="post">
            <div class="form-group">
                <label asp-for="Output.NazivPosla"></label>
                <input asp-for="Output.NazivPosla" class="form-control" disabled />
            </div>
            <div class="form-group">
                <label asp-for="Output.Adresa"></label>
                <input asp-for="Output.Adresa" class="form-control" disabled />
            </div>
            <div class="form-group">
                <label asp-for="Output.Grad"></label>
                <input asp-for="Output.Grad" class="form-control" disabled />
            </div>
            <div class="form-group">
                <label asp-for="Output.Opis"></label>
                <input asp-for="Output.Opis" class="form-control" disabled />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}